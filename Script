-- SHINDO

local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Shindo", "Sentinel")

local Scripts = Window:NewTab("Scripts")
local Scripts = Scripts:NewSection("Scripts")
Scripts:NewButton("Spy Hub", " ", function()
    loadstring(game:HttpGet("https://pastebin.com/raw/mVTanGiF"))()
end)
Scripts:NewButton("Bruh Hub", " ", function()
    loadstring(game:HttpGet("https://bruh.keshsenpai.com/.lua"))()
end)
Scripts:NewButton("Solaris Hub (NEW!)", " ", function()
    loadstring(game:HttpGet("https://solarishub.dev/script.lua"))()
end)
